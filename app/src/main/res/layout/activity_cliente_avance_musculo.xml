<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ClienteAvanceMusculo">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerHorizontal="true"
        android:weightSum="4"
        android:orientation="vertical"
        android:layout_alignParentBottom="true">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:weightSum="2"
            android:layout_weight="1">
            <android.support.v7.widget.CardView
                android:id="@+id/btnBrazos_ClienteAvanceMusculo"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="10dp"
                android:clickable="true"
                android:foreground="?android:selectableItemBackground"
                android:layout_weight="1">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="bottom">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@drawable/brazo"
                        android:layout_gravity="center" />





                </LinearLayout>
                <TextView
                    android:id="@+id/txtvBrazo_ClienteAvanceMusculo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textStyle="bold"
                    android:text="Brazo y antebrazo"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:background="#16000000"
                    android:textSize="20dp"
                    android:padding="10dp"
                    android:textColor="@color/negro"/>


            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:id="@+id/btnHombros_ClienteAvanceMusculo"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="10dp"
                android:clickable="true"
                android:foreground="?android:selectableItemBackground"
                android:layout_weight="1">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="bottom">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@drawable/hombros"
                        android:layout_gravity="center"
                        />




                </LinearLayout>
                <TextView
                    android:id="@+id/txtvHombros_ClienteAvanceMusculo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textStyle="bold"
                    android:text="Hombros"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:background="#16000000"
                    android:textSize="20dp"
                    android:padding="10dp"
                    android:textColor="@color/negro"
                    />


            </android.support.v7.widget.CardView>


        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:weightSum="2"
            android:layout_weight="1">
            <android.support.v7.widget.CardView
                android:id="@+id/btnEspalda_ClienteAvanceMusculo"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="10dp"
                android:clickable="true"
                android:foreground="?android:selectableItemBackground"
                android:layout_weight="1">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:fitsSystemWindows="true"
                        android:src="@drawable/espalda"
                        android:layout_gravity="center" />





                </LinearLayout>

                <TextView
                    android:id="@+id/txtvEspalda_ClienteAvanceMusculo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textStyle="bold"
                    android:text="Espalda"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:background="#16000000"
                    android:textSize="20dp"
                    android:padding="10dp"
                    android:textColor="@color/negro"
                    />

            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:id="@+id/btnPiernas_ClienteAvanceMusculo"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="10dp"
                android:clickable="true"
                android:foreground="?android:selectableItemBackground"
                android:layout_weight="1">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="bottom">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@drawable/pierna"
                        android:layout_gravity="center" />





                </LinearLayout>

                <TextView
                    android:id="@+id/txtvPiernas_ClienteAvanceMusculo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textStyle="bold"
                    android:text=" Piernas"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:background="#16000000"
                    android:textSize="20dp"
                    android:padding="10dp"
                    android:textColor="@color/negro"/>

            </android.support.v7.widget.CardView>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:weightSum="2"
            android:layout_weight="1">
            <android.support.v7.widget.CardView
                android:id="@+id/btnGluteos_ClienteAvanceMusculo"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="10dp"
                android:clickable="true"
                android:foreground="?android:selectableItemBackground"
                android:layout_weight="1">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="bottom">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@drawable/gluteos"
                        android:layout_gravity="center" />



                </LinearLayout>

                <TextView
                    android:id="@+id/txtvGluteos_ClienteAvanceMusculo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textStyle="bold"
                    android:text="GlÃºteos"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:background="#16000000"
                    android:textSize="20dp"
                    android:padding="10dp"
                    android:textColor="@color/negro"/>

            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:id="@+id/btnAbdominales_ClienteAvanceMusculo"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="10dp"
                android:clickable="true"
                android:foreground="?android:selectableItemBackground"
                android:layout_weight="1">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="bottom">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@drawable/abdominales"
                        android:layout_gravity="center"/>




                </LinearLayout>

                <TextView
                    android:id="@+id/txtvCerrarSesion_ClienteAvanceMusculo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textStyle="bold"
                    android:text="Abdominales"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:background="#16000000"
                    android:textSize="20dp"
                    android:padding="10dp"
                    android:textColor="@color/negro"/>

            </android.support.v7.widget.CardView>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:weightSum="2"
            android:layout_weight="1"
            android:gravity="center">
            <android.support.v7.widget.CardView
                android:id="@+id/btnPectorales_ClienteAvanceMusculo"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="10dp"
                android:clickable="true"
                android:foreground="?android:selectableItemBackground"
                android:layout_weight="1">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="bottom">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@drawable/pectorales"
                        android:layout_gravity="center"/>

                    
                </LinearLayout>
                <TextView
                    android:id="@+id/txtvActualizarContra_ClienteAvanceMusculo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textStyle="bold"
                    android:text="Pectorales"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:background="#16000000"
                    android:textSize="20dp"
                    android:padding="10dp"
                    android:textColor="@color/negro"/>

            </android.support.v7.widget.CardView>
            
        </LinearLayout>

        
    </LinearLayout>
    
</RelativeLayout>