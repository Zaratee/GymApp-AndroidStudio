<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".InstructorClienteRelizSele">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_gravity="center"
        android:gravity="center">

            <android.support.v7.widget.CardView
                android:id="@+id/btnGraficaRealizado_InstructorEditarDia"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:clickable="true"
                android:foreground="?android:selectableItemBackground">
                <LinearLayout
                    android:layout_width="350dp"
                    android:layout_height="150dp"
                    android:orientation="vertical"
                    android:gravity="center">

                    <TextView
                        android:id="@+id/txtvGraficaRealizado_InstructorEditarDia"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="GrÃ¡fica por ejercicio"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:background="#16000000"
                        android:textSize="20dp"
                        android:padding="10dp"
                        android:textColor="@color/negro"/>



                </LinearLayout>

            </android.support.v7.widget.CardView>








            <android.support.v7.widget.CardView
                android:id="@+id/btnListRealiz_InstructorEditarDia"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:clickable="true"
                android:foreground="?android:selectableItemBackground">
                <LinearLayout
                    android:layout_width="350dp"
                    android:layout_height="150dp"
                    android:orientation="vertical"
                    android:gravity="center">

                    <TextView
                        android:id="@+id/txtvListRealiz_InstructorEditarDia"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:text="Lista realizado"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:background="#16000000"
                        android:textSize="20dp"
                        android:padding="10dp"
                        android:textColor="@color/negro"/>

                </LinearLayout>

            </android.support.v7.widget.CardView>

        </LinearLayout>

        </LinearLayout>














